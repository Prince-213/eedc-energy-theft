generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Users {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  address   String
  april     Int
  august    Int
  december  Int
  febuary   Int
  january   Int
  july      Int
  june      Int
  march     Int
  may       Int
  meter     String
  name      String
  november  Int
  october   Int
  september Int
  status    String
}
